@model Arasan.Models.ItemCategory

@{
    ViewData["Title"] = "Category";
}
<div class="row wrapper border-bottom page-heading">
    <div class="col-lg-12">

        <ol class="breadcrumb">
            <li> <a href="#">Home</a> </li>
            <li> <a>Master</a> </li>
            <li class="active"> <strong>Item Category</strong> </li>
        </ol>
    </div>
</div>

<div class="wrapper-content ">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="widgets-container">
                  
                    <hr>
                       @using (Html.BeginForm("ItemCategory", "ItemCategory", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                    <form>
                        
                            <div class="row">

                                <div class="col-lg-3"></div>
                                <div class="col-md-6">
                                    <div class="form-group">

                                        <label for="inputName3" class="col-sm-4 control-label">Category<label class="Mandatory"></label></label>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(C => C.Category, new { @id = "txtCategory", @class = "form-control", @TabIndex = 1 })<br />

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="height:30px;"></div>
                        <div class="col-lg-8"></div>
                            <button type="submit" name="submit" class="btn aqua m-t-xs bottom15-xs" id="btnSave" value="Save" tabindex="18">Save</button>
                            <a button class="btn btn-danger" type="submit" href="@Url.Action("ListItemCategory", "ItemCategory")">Cancel</a>
                      
                    </form>
                    }
                </div>
            </div>
        </div>
    </div>
</div>