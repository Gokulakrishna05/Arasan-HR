@model Arasan.Models.Machine

@{
    ViewData["Title"] = "Machine";
}
<style>

    .mandatory {
        color: red;
        position: relative;
        top: 2px;
        left: 5px;
    }
</style>
<script type="text/javascript">
    @if (TempData["notice"] != null)
    {
        @:alert('@TempData["notice"]');
    }
</script>

<div class="row wrapper border-bottom page-heading">
    <div class="col-lg-12">

        <ol class="breadcrumb">
            <li> <a href="#">Home</a> </li>
            <li> <a href="@Url.Action("MachineList", "Machines")">Machines</a> </li>
            <li class="active"> <strong>Machines</strong> </li>
        </ol>
    </div>
</div>

<style>
    .tab-content > .tab-pane {
        display: none;
        padding: 10px !important;
    }

    .mandatory {
        color: red;
        position: relative;
        top: 2px;
        left: 5px;
    }
</style>
<div class="wrapper-content">
    <div class="row">
