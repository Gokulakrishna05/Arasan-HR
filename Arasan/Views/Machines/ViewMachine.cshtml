                                                    @Html.DisplayFor(C => C.MCYear, new { @id = "MCYear", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
