@model Arasan.Models.Machine

@{
    Layout = "_PopupLayout";
    ViewData["Title"] = "Machine";
}
<style>

    .mandatory 
    {
        color: red;
        position: relative;
        top: 2px;
        left: 5px;
    }
</style>
<script type="text/javascript">
    @if (TempData["notice"] != null)
    {
        @:alert('@TempData["notice"]');
    }
</script>

<div class="row wrapper border-bottom page-heading">
    <div class="col-lg-12">

        <ol class="breadcrumb">
            <li> <a href="#">Home</a> </li>
            <li> <a href="@Url.Action("MachineList", "Machines")">Machine</a> </li>
            <li class="active"> <strong>View Machines</strong> </li>
        </ol>
    </div>
</div>

<style>
    .tab-content > .tab-pane {
        display: none;
        padding: 10px !important;
    }

    .mandatory {
        color: red;
        position: relative;
        top: 2px;
        left: 5px;
    }
</style>
<div class="wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="widgets-container">
                    <h5 style="font-size:14px">Machine</h5>
                    <hr>


                    <form>
                        <div class="tabs-container">
                            <ul class="nav nav-tabs">
                                <li class="active"></li>
                                <li><a href="#tab-1" data-toggle="tab">Machine Information</a></li>
                                <li><a href="#tab-2" data-toggle="tab">Technical Info</a></li>
                                <li><a href="#tab-3" data-toggle="tab">Important Dates</a></li>
                                <li><a href="#tab-4" data-toggle="tab">Cost Details </a></li>
                                <li><a href="#tab-5" data-toggle="tab">Components Details</a></li>
                                <li><a href="#tab-6" data-toggle="tab">Major Parts</a></li>
                                <li><a href="#tab-7" data-toggle="tab">Checklist Details></a></li>

                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane active" id="tab-1">
                                    @*1st atb*@
                                    <br>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />   <label for="inputName3" class="col-sm-4 control-label">Machine Id<label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MId, new { @id = "MId", @class = "form-control", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />   <label for="inputName3" class="col-sm-4 control-label">Machine Name<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MName, new { @id = "MName", @class = "form-control", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>



                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Work Center<label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MWrkCent, new { @id = "MWrkCent", @Class = "form-control _select", @TabIndex = 1 })
                                                    @* @Html.HiddenFor(C => C.ID)
                                                    @Html.HiddenFor(C => C.createdby) *@
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Supplied By<label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MSupply, new { @id = "MSupply", @class = "form-control", @TabIndex = 1 })

                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Sub Process Id<label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MSubProc, new { @id = "MSubProc", @Class = "form-control _select", @TabIndex = 1 })
                                                    @Html.HiddenFor(C => C.ID)
                                                    @* @Html.HiddenFor(C => C.createdby) *@
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />  <label for="inputName3" class="col-sm-4 control-label">Serial Number<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MSerial, new { @id = "MSerial", @class = "form-control", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />   <label for="inputName3" class="col-sm-4 control-label">Machine Model<label class="Mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MModel, new { @id = "MModel", @class = "form-control", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />  <label for="inputName3" class="col-sm-4 control-label">Manufacturer Name<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MManname, new { @id = "MManname", @class = "form-control", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br />  <label for="inputName3" class="col-sm-4 control-label">Made In <label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MMade, new { @id = "MMade", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Mode Of Purchase <label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MPur, new { @id = "MPur", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Service Support By<label class="Mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MSer, new { @id = "MSer", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Service Company<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MSerCmp, new { @id = "MSerCmp", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Machine Type <label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MMType, new { @id = "MMType", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Machine Incharge <label class="Mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MIncharge, new { @id = "MIncharge", @class = "form-control", @TabIndex = 1, @maxlength = 1 })
                                                </div>

                                            </div>
                                        </div>

                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br />   <label for="inputName3" class="col-sm-4 control-label">Auxiliary<label class="mandatory"></label></label>
                                                <div class="col-sm-8">

                                                        @Html.DisplayFor(C => C.Aux)
                                                      
                                                    </div>
                                               

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Used To<label class="Mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MUse, new { @id = "MUse", @class = "form-control", @TabIndex = 1, @maxlength = 1 })
                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Machine Mode<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MMMode, new { @id = "MMMode", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Location<label class="mandatory">*</label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MLoc, new { @id = "MLoc", @Class = "form-control _select", @TabIndex = 1 })

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>







                                <div class="tab-pane active" id="tab-2">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Machine Capascity<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MCap, new { @id = "MCap", @class = "form-control", @TabIndex = 1 })

                                                    @Html.DisplayFor(C => C.MCapUnit, new { @id = "MCapUnit", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Estimated Life<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MELife, new { @id = "MELife", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MELifeUnit, new { @id = "MELifeUnit", @Class = "form-control _select", @TabIndex = 1 })

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Power Consumption Per Hour<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MPower, new { @id = "MPower", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MPowerUnit, new { @id = "MPowerUnit", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Power Utilization Factor<label class="mandatory"></label></label>
                                                <div class="col-sm-8">

                                                    @Html.DisplayFor(C => C.MPFactor, new { @id = "MPFactor", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Cum.Run Hours<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MRun, new { @id = "MRun", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MRunUnit, new { @id = "MRunUnit", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">So.Far Run Hours<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MRunH, new { @id = "MRunH", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MRunHUnit, new { @id = "MRunHUnit", @Class = "form-control _select", @TabIndex = 1 })

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Lead Hours<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MLead, new { @id = "MLead", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MLeadUnit, new { @id = "MLeadUnit", @Class = "form-control _select", @TabIndex = 1 })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">

                                                <br /><label for="inputName3" class="col-sm-4 control-label">Maintenance Houre<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MMaintain, new { @id = "MMaintain", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })

                                                    @Html.DisplayFor(C => C.MMaintainUnit, new { @id = "MMaintainUnit", @Class = "form-control _select", @TabIndex = 1 })

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>





                                <div class="tab-pane" id="tab-3">
                                    @*3rd tab*@

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label"> Date Of Purchase<label class="mandatory"></label></label>
                                                <div class="input-group date form_date col-sm-8" data-date="" data-date-format="dd-M-yyyy">

                                                    @Html.DisplayFor(C => C.DOP, new { @id = "DOP", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span> <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    <br />

                                                </div>
                                            </div>
                                        </div>
                                    </div>





                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Date Of Supply<label class="mandatory"></label></label>
                                                <div class="input-group date form_date col-sm-8" data-date="" data-date-format="dd-M-yyyy">

                                                    @Html.DisplayFor(C => C.DOS, new { @id = "DOS", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span> <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    <br />

                                                </div>

                                            </div>
                                        </div>
                                    </div>



                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Installation Date<label class="mandatory"></label></label>
                                                <div class="input-group date form_date col-sm-8" data-date="" data-date-format="dd-M-yyyy">

                                                    @Html.DisplayFor(C => C.InsDate, new { @id = "InsDate", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span> <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    <br />

                                                </div>

                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Date Of Last Maintenance<label class="mandatory"></label></label>
                                                <div class="input-group date form_date col-sm-8" data-date="" data-date-format="dd-M-yyyy">

                                                    @Html.DisplayFor(C => C.DOLMain, new { @id = "DOLMain", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span> <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    <br />

                                                </div>

                                            </div>


                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Next Mintenance Date<label class="mandatory"></label></label>
                                                <div class="input-group date form_date col-sm-8" data-date="" data-date-format="dd-M-yyyy">

                                                    @Html.DisplayFor(C => C.NMainDate, new { @id = "NMainDate", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span> <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    <br />

                                                </div>

                                            </div>



                                        </div>
                                    </div>


                                    @* 3rd tab*@
                                </div>



                                <div class="tab-pane" id="tab-4">
                                    @*4th tab*@
                                    <div class="row">

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Machine Landed Cost<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.MLCost, new { @id = "MLCost", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label"> Depreciation %<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.Dep, new { @id = "Dep", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Interest %<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.Int, new { @id = "Int", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <br /> <label for="inputName3" class="col-sm-4 control-label">Salary Operators/Years<label class="mandatory"></label></label>
                                                <div class="col-sm-8">
                                                    @Html.DisplayFor(C => C.SOYear, new { @id = "SOYear", @class = "form-control", @TabIndex = 1, onkeydown = "return isNumeric2(event.keyCode,this.value)" })
                                                </div>

                                            </div>
                                        </div>
                                    </div>

