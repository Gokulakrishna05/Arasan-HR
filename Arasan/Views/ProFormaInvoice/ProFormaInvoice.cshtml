    $(function () {
        var ItemId = '@ViewBag.WorkCenter';
        changeItem(ItemId);
    });
    function changeItem(ItemId) 
    {
        document.getElementById("tbBooks").innerHTML = "";
        $.ajax({
            url: 'GetWorkOrderDetails',
            type: "post",
            data: { "id": ItemId },
            cache: false,

            success: function (list) {
                //if (list != null && list.length > 0) {
                list.forEach((item) => {

                    var index = $("#tbBooks").children("tr").length;
                    //alert(item.batch);
                    var Item = "<td><input class='form-control' id='txtItem" + index + "' name='ProFormalst[" + index + "].itemid'  style='width: 100px;' type='text'  readonly='True'  value=" + item.itemid + " /></td>";

                    var ItemDes = "<td><input class='form-control' id='txtitemdes" + index + "' name='ProFormalst[" + index + "].itemdes'  style='width: 100px;' type='text'  readonly='True'  value=" + item.itemdes + " /></td>";

                    var Unit = "<td><input class='form-control' id='txtunit" + index + "' name='ProFormalst[" + index + "].unit'  style='width: 100px;' type='text'  readonly='True'  value=" + item.unit + " /></td>";


                    var Qty = "<td><input class='form-control' id='txtqty" + index + "' name='ProFormalst[" + index + "].qty'  style='width: 80px;' type='text' readonly='True' value=" + item.qty + " onkeyup = 'changeQty(this," + index + ")'  /></td>";

                    var Rate = "<td><input class='form-control' id='txtrate" + index + "' name='ProFormalst[" + index + "].rate'  style='width: 60px;' type='text' readonly='True'  value=" + item.rate + " onkeyup = 'changeQty(this," + index + ")' /></td>";

                   
                    var Amount = "<td><input class='form-control' id='txtamount" + index + "' name='ProFormalst[" + index + "].amount'  style='width: 100px;' type='text' readonly='True'  value=" + item.amount + " onkeyup = 'changeQty(this," + index + ")'   /></td>";

                    var Discount = "<td><input class='form-control' id='txtdiscount" + index + "' name='ProFormalst[" + index + "].discount'  style='width: 100px;' type='text'  readonly='True'  value=" + item.discount + " onkeyup = 'changeQty(this," + index + ")'   /></td>";


                    var ItroDiscount = "<td><input class='form-control' id='txtitrodis" + index + "' name='ProFormalst[" + index + "].itrodis'  style='width: 100px;' type='text'    readonly='True'  value=" + item.itrodis + " onkeyup = 'changeQty(this," + index + ")' /></td>";

                    var CashDiscount = "<td><input class='form-control' id='txtcashdisc" + index + "' name='ProFormalst[" + index + "].cashdisc'  style='width: 100px;' type='text'   readonly='True'  value=" + item.cashdisc + " onkeyup = 'changeQty(this," + index + ")' /></td>";

                    var TradeDiscount = "<td><input class='form-control' id='txttradedis" + index + "' name='ProFormalst[" + index + "].tradedis'  style='width: 100px;' type='text'   readonly='True'  value=" + item.tradedis + " onkeyup = 'changeQty(this," + index + ")'   /></td>";

                    var AdditionalDiscount = "<td><input class='form-control' id='txtadditionaldis" + index + "' name='ProFormalst[" + index + "].additionaldis'  style='width: 100px;' type='text'    readonly='True'  value=" + item.additionaldis + " onkeyup = 'changeQty(this," + index + ")'  /></td>";

                   
                    var Specialdiscount = "<td><input class='form-control' id='txtdis" + index + "' name='ProFormalst[" + index + "].dis'  style='width: 100px;' type='text'   readonly='True'  value=" + item.dis + " onkeyup = 'changeQty(this," + index + ")' /></td>";

                    var Frieght = "<td><input class='form-control' id='txtfrieght" + index + "' name='ProFormalst[" + index + "].frieght'  style='width: 100px;' type='text' readonly='True'  value=" + item.frieght + "  onkeyup = 'changeQty(this," + index + ")' /></td>";


                    var Tariff = "<td><input class='form-control' id='txttariff" + index + "' name='ProFormalst[" + index + "].tariff'  style='width: 100px;' type='text'    /></td>";

                    var CGSTP = "<td><input class='form-control' id='txtCGSTP" + index + "' name='ProFormalst[" + index + "].CGSTP'  style='width: 100px;' type='text'   onkeyup = 'changeQty(this," + index + ")'  /></td>";

                    var SGSTP = "<td><input class='form-control' id='txtSGSTP" + index + "' name='ProFormalst[" + index + "].SGSTP'  style='width: 100px;' type='text'  onkeyup = 'changeQty(this," + index + ")'  /></td>";

                    var IGSTP = "<td><input class='form-control' id='txtIGSTP" + index + "' name='ProFormalst[" + index + "].IGSTP'  style='width: 100px;' type='text'  onkeyup = 'changeQty(this," + index + ")'   /></td>";

                    var CGST = "<td><input class='form-control' id='txtCGST" + index + "' name='ProFormalst[" + index + "].CGST'  style='width: 100px;' type='text' readonly='True'  onkeyup = 'changeQty(this," + index + ")'  /></td>";

                    var SGST = "<td><input class='form-control' id='txtSGST" + index + "' name='ProFormalst[" + index + "].SGST'  style='width: 100px;' type='text'  readonly='True'  onkeyup = 'changeQty(this," + index + ")' /></td>";

                    var IGST = "<td><input class='form-control' id='txtIGST" + index + "' name='ProFormalst[" + index + "].IGST'  style='width: 100px;' type='text'   readonly='True' onkeyup = 'changeQty(this," + index + ")' /></td>";

                   
                    var TotAmount = "<td><input class='form-control' id='txttotamount" + index + "' name='ProFormalst[" + index + "].totamount'  style='width: 100px;' type='text' readonly='True'      /></td>";


                    var newRow = "<tr class='chkitemlst'  style='height: 30px;border-bottom: 1px dashed #ddd;'  id='trBook" + index + "'>" + Item + ItemDes + Unit + Qty + Rate + Amount + Discount + ItroDiscount + CashDiscount + TradeDiscount + AdditionalDiscount + Specialdiscount + Frieght + Tariff + CGSTP + SGSTP + IGSTP + CGST + SGST + IGST + TotAmount + "</tr>";

                    $("#tbBooks").append(newRow);
                });
                //}
            },
            error: function () {
                alert("Data Not Found");
            }
        });
        $.ajax({
            url: 'GetProFormaInvoiceDetail',
            data: { "ItemId": ItemId },
            type: "GET",
            success: function (result) {
                
                document.getElementById("txtCurrency").value = result.currency;
                document.getElementById("txtParty").value = result.party;
              
               
            },
            error: function () {
                alert("Data Not Found");
            }
        });
    }
    function changeQty(_this, i) 
    {
        try {
            var GTot = 0;
            var j = 0;
            var qty = 0;
            var tot = 0;
            var totfrieght = 0;
            var totamt = 0;
            var totamount = 0;
            var qty = document.getElementById("txtqty" + i).value;
            var up = document.getElementById("txtrate" + i).value;
            var discount = document.getElementById("txtDisc" + i).value;
            var itdis = document.getElementById("txtitrodis" + i).value;
            var cdis = document.getElementById("txtcashdisc" + i).value;
            var tdis = document.getElementById("txttradedis" + i).value;
            var adis = document.getElementById("txtadditionaldis" + i).value;
            var disc = document.getElementById("txtdis" + i).value;
            var frieght = document.getElementById("txtfrieght" + i).value;
            var IGST = document.getElementById("txtIGSTP" + i).value;

            var CGST = document.getElementById("txtCGSTP" + i).value;

            var SGST = document.getElementById("txtSGSTP" + i).value;
            
            if (qty == "")
                qty = 0;

            if (up == "")
                up = 0;

              if (discount == "")
                discount = 0;
           
				
            if (itdis == "")
                itdis = 0;
				
            if (cdis == "")
                cdis = 0;
				
            if (tdis == "")
                tdis = 0;
				
            if (adis == "")
                adis = 0;
			
			if (disc == "")
                disc = 0;

            if (IGST == "")
                IGST = 0;


            if (CGST == "")
                CGST = 0;


            if (SGST == "")
                SGST = 0;

            

            if (frieght == "")
                 frieght = 0;

             var tot = eval(qty) * eval(up) ;
             document.getElementById("txttotamount" + i).value = tot.toFixed(2);
             var igstperc = (eval(tot) / 100) * eval(IGST);
             var cgstperc = (eval(tot) / 100) * eval(CGST);
             var sgstperc = (eval(tot) / 100) * eval(SGST);
			 var Discperc = (eval(tot) / 100) * eval(discount);
			 var itdiscount = (eval(tot) / 100) * eval(itdis);
			 var cashdiscount = (eval(tot) / 100) * eval(cdis);
			 var tradediscount = (eval(tot) / 100) * eval(tdis);
			 var adddiscount = (eval(tot) / 100) * eval(adis);
			 var idiscount = (eval(tot) / 100) * eval(disc);
			
            

            document.getElementById("txtCGST" + i).value = cgstperc.toFixed(2);
            document.getElementById("txtSGST" + i).value = sgstperc.toFixed(2);
             document.getElementById("txtIGST" + i).value = igstperc.toFixed(2);
            document.getElementById("txtdiscount" + i).value = Discperc.toFixed(2);
            document.getElementById("txtitrodis" + i).value = itdiscount.toFixed(2);
            document.getElementById("txtcashdisc" + i).value = cashdiscount.toFixed(2);
            document.getElementById("txttradedis" + i).value = tradediscount.toFixed(2);
            document.getElementById("txtadditionaldis" + i).value = adddiscount.toFixed(2);
            document.getElementById("txtdis" + i).value = idiscount.toFixed(2);

            var tottax = eval(tot) + eval(igstperc) + eval(cgstperc) + eval(sgstperc) + eval(frieght);
            var totdisc = eval(tottax) - eval(Discperc) - eval(itdiscount) - eval(cashdiscount) - eval(tradediscount) - eval(adddiscount) - eval(txtdis) ;
          

            document.getElementById("txttotamount" + i).value = totdisc.toFixed(2);

            $('tr.chkitemlst').each(function () {
                if ($(this).css('display') !== 'none') {
                    frieght = $(this).find('#txtfrieght' + j.toString()).val();
                    if (frieght == "")
                        frieght = 0;
                    var amt = $(this).find('#txttotamount' + j.toString()).val();
                    if (amt == "")
                        amt = 0;

                    totamt += eval(amt);
                     totfrieght += eval(frieght);
                    var tamt = $(this).find('#txttotamount' + j.toString()).val();
                            if (tamt == "")
                        tamt = 0;

                        totamount += eval(tamt);

                              }
                        j += 1;
                    });

            document.getElementById("txtFrig").value = totfrieght.toFixed(2);
            document.getElementById("Gross").value = totamt.toFixed(2);
            document.getElementById("Net").value = totamount.toFixed(2);
            document.getElementById("hidnet").value = totamount.toFixed(2);

        }
        catch (ex) {
            alert(ex.message);
        }
    }
    function removeRow(id) {
        var table = document.getElementById("dataTable");
        var rowCount = table.rows.length;
        var row = document.getElementById('trBook' + id);
        var j = 0;
        $('tr.chkitemlst').each(function () {
            if ($(this).css('display') !== 'none') {
                j += 1;
            }
        });
        if (j > 1) {
            row.style.display = "none";
            document.getElementById('Isvalid' + id).value = "N";
        }
        else {
            alert("Cannot delete all the rows.")
        }
    }
    function PopulateDropDown(dropDownId, list) {
        //alert(list);
        $(dropDownId).empty().append('<option selected="selected" value="0">Please select</option>');
        if (list != null && list.length > 0) {
            $(dropDownId).removeAttr("disabled");
            $.each(list, function () {
                $(dropDownId).append($("<option></option>").val(this['value']).html(this['text']));
            });
        }
        $(dropDownId).trigger("liszt:updated");
    }
