                                    <label for="inputName3" class="col-sm-4 control-label">Prod.Sieve<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                                                              @Html.TextBoxFor(C => C.ProdSieve, new { @id = "txtProdSieve", @class = "form-control", @TabIndex = 1 })<br />
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">

                                    <label for="inputName3" class="col-sm-4 control-label">Fuel Qty<label class="Mandatory">*</label></label>
