    function AppendRow(opt) {
        var index = $("#tbBooks").children("tr").length;

         var ItemGrp = "<td><select style = 'width: 170px;' class='form-control chosen-select newcss' id='ddlItemGrp" + index + "' name='PrsLst[" + index + "].ItemGroupId' onchange = 'Grup_Change(this.value," + index + ")'><option value=''>Please select</option>" + opt + "</select></td>";

        var Item = "<td><select style = 'width: 170px;' class='form-control chosen-select newcss' id='ddlItem" + index + "' name='PrsLst[" + index + "].ItemId' onchange = 'changeItem(this.value," + index + ")'><option value=''>Please select</option></select></td>";

        var Desc = "<td><input class='form-control' id='txtDesc" + index + "' name='PrsLst[" + index + "].Desc'  style='width: 200px;' type='text'  /></td>";

        var Unit = "<td><input class='form-control' id='txtUnit" + index + "' name='PrsLst[" + index + "].Unit'  style='width: 100px;' type='text'  readonly='True' /></td>";

        var Input = "<td><input class='form-control' id='txtInput" + index + "' name='PrsLst[" + index + "].Input'  style='width: 120px;' type='text'   /></td>";

        var Qty = "<td><input class='form-control' id='txtQty" + index + "' name='PrsLst[" + index + "].Qty'  style='width: 120px;' type='text'  /></td>";

        var removeCell = "<td><input id='Isvalid" + index + "' name='PrsLst[" + index + "].Isvalid' type='hidden' value='Y'><img  src='../Images/delete.png' onclick='removeRow(" + index + ");'  style='cursor:pointer' /><input name='EnqLst.Index' type='hidden' value='" + index + "'  /></td>";
       

        var newRow = "<tr class='chkitemlst'  style='height: 30px;border-bottom: 1px dashed #ddd;'  id='trBook" + index + "'>" + ItemGrp + Item + Desc + Unit + Input + Qty + removeCell + "</tr>";
