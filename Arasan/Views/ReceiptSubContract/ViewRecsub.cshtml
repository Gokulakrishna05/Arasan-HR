                                <div class="col-md-6">
                                    <div class="form-group">

                                        <label for="inputName3" class="col-sm-4 control-label">Ref Date<label class="Mandatory"></label></label>

                                        <div class="col-sm-8">
                                    @Html.DisplayFor(C => C.RefDate) <br />


                                        <br />

                                    </div>


                                    </div>
                                </div>
                            </div>

