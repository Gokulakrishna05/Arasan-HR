                                            <li><a href="#tab-2" data-toggle="tab">Item to be Delivered </a></li>

                                        </ul>
                                        <div class="tab-content">
                                            <div class="tab-pane active" id="tab-1">
                                                <!-- 1st tab  -->
                                                @* <div class="col-md-12">
                                            <div class="btn-group">
                                            <button class="btn sbold green" id="addRow"> Add New <i class="fa fa-plus"></i> </button>
                                            </div>
                                            </div>*@

                                                <div class="col-lg-12">
                                                    <div class="col-md-12" style="overflow-x:scroll; min-width:300px; min-height:300px;">
                                                        <!-- / ibox-title -->
                                                        <table class="table table-striped table-hover" id="dataTable">
                                                            <thead>
                                                                <tr>

                                                                    <th>Exp.Rec.Item</th>
                                                                    <th>Drum</th>
                                                                    <th>Unit ID</th>
                                                                    <th>Exp.Rec.Qty</th>
                                                                    <th>Rate</th>
                                                                    <th>Amount</th>

                                                                </tr>
                                                            </thead>
                                                            <tbody id="tbBooks1">
                                                                @{
                                                                    int i = 0;

                                                                    foreach (var item in Model.Delilst.ToList())
                                                                    {

                                                                        <tr id="trBook@(i)" class="chkitemlst" style="height: 30px;border-bottom: 1px dashed #ddd;">

                                                                            <td>
                                                                            @Html.DisplayFor(o => o.Delilst[i].item)
                                                                            @Html.HiddenFor(o => o.Delilst[i].supid)
                                                                        </td>
                                                                            <td><div class='fa-hover col-md-2 col-sm-4'> <a href='@Url.Action("ViewDrum", "ReceiptSubContract",new{id= item.supid})' class='fancybox' data-fancybox-type="iframe"><img src='../Images/view_icon.png' /></a></div> </td>
                                                                        <td> @Html.DisplayFor(o => o.Delilst[i].unit) </td>
                                                                        <td> @Html.DisplayFor(o => o.Delilst[i].qty) </td>
                                                                        <td> @Html.DisplayFor(o => o.Delilst[i].rate) </td>
                                                                        <td> @Html.DisplayFor(o => o.Delilst[i].amount) </td>
                                                                           

                                                                        </tr>
                                                                        i++;
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>

                                                <!-- 1st tab  -->
                                            </div>
                                            <div class="tab-pane" id="tab-2">
                                                <!-- 2nd tab  -->
                                                @* <div class="col-md-12">
                                            <div class="btn-group">
                                            <button class="btn sbold green" id="addRow"> Add New <i class="fa fa-plus"></i> </button>
                                            </div>
                                            </div>*@

                                                <div class="col-lg-12">
                                                    <div class="col-md-12" style="overflow-x:scroll; min-width:300px; min-height:300px;">
                                                        <!-- / ibox-title -->
                                                        <table class="table table-striped table-hover" id="dataTable">
                                                            <thead>
                                                                <tr>

                                                                    <th>Item ID</th>
                                                                    <th>Unit</th>
                                                                    <th>Qty</th>
                                                                    <th>Rate</th>
                                                                    <th>Amount</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody id="tbBooks">
                                                                @{
                                                                    int j = 0;

                                                                    foreach (var item in Model.Reclst.ToList())
                                                                    {

                                                                        <tr id="trBook@(j)" class="chkitemlst" style="height: 30px;border-bottom: 1px dashed #ddd;">

                                                                            <td>
                                                                            @Html.DisplayFor(o => o.Reclst[j].item)
                                                                        </td>
                                                                        <td> @Html.DisplayFor(o => o.Reclst[j].unit)  </td>
                                                                        <td> @Html.DisplayFor(o => o.Reclst[j].qty)  </td>
                                                                        <td> @Html.DisplayFor(o => o.Reclst[j].rate)  </td>
                                                                        <td> @Html.DisplayFor(o => o.Reclst[j].amount) </td>
                                                                          

                                                                        </tr>
                                                                        j++;
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>


                                                <!-- 2nd tab  -->
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>



                            <div style="height:30px;"></div>
                            <div class="row">

                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="inputName3" class="col-sm-4 control-label">Qty to be Recived<label class="Mandatory"></label></label>

                                        <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.qtyrec) <br />
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">

                                        <label for="inputName3" class="col-sm-4 control-label">Recived Qty<label class="Mandatory"></label></label>
                                        <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.TotRecqty) <br />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">


                                <div class="col-md-6">
                                    <div class="form-group">

                                    <br /><label for="inputName3" class="col-sm-4 control-label">Narration<label class="Mandatory"></label></label>
                                        <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.Narr) <br />
                                        </div>
                                    </div>
                                </div>
                            </div>
 
                        </form>
                    
                </div>
            </div>
        </div>

    </div>
</div>



