                                        <label for="inputName3" class="col-sm-4 control-label">D.C.No<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.DCNo) <br />


                                        

                                    </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="inputName3" class="col-sm-4 control-label">Recived From<label class="Mandatory"></label></label>

                                        <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.Supplier) <br />
                                    </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">

                                        <br />  <label for="inputName3" class="col-sm-4 control-label">Add 1<label class="Mandatory"></label></label>
                                        <div class="col-sm-8">
      @Html.DisplayFor(C => C.Add1) <br />                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <br /> <label for="inputName3" class="col-sm-4 control-label">Add 2<label class="Mandatory"></label></label>

                                        <div class="col-sm-8">
                                                 @Html.DisplayFor(C => C.Add2) <br />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">

                                    <br />  <label for="inputName3" class="col-sm-4 control-label">City<label class="Mandatory"></label></label>
                                        <div class="col-sm-8">
                                                  @Html.DisplayFor(C => C.City) <br />
                                        </div>
                                    </div>
                                </div>
