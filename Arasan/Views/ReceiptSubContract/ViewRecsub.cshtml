                                        <div class="col-sm-8">
                                          
                                        @Html.DisplayFor(C => C.DocNo) <br />
                                            @Html.HiddenFor(C => C.ID)
                                            @Html.HiddenFor(C => C.Branch)
                                            @Html.HiddenFor(C => C.enterd)
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="inputName3" class="col-sm-4 control-label">Doc Date<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                    @Html.DisplayFor(C => C.DocDate) <br />


                                        <br />

                                    </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="inputName3" class="col-sm-4 control-label">Ref No<label class="Mandatory"></label></label>

                                        <div class="col-sm-8">
                                        @Html.DisplayFor(C => C.RefNo) <br />
                                    </div>
                                    </div>
