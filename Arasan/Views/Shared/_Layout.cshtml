<script>
    $(function () {
        var url = window.location.href;
        var rest = url.substring(0, url.lastIndexOf("/") + 1);
        $('.nav-item.active, .nav-link.active').removeClass('active');
        $('.nav-item.open').removeClass('open');
       
        document.querySelectorAll(".nav-link").forEach((link) => {
            var navrest = link.href.substring(0, url.lastIndexOf("/") + 1);
            if (navrest === rest) {
                link.classList.add("active");
                link.parentElement.classList.add("active");
                link.parentElement.parentElement.parentElement.classList.add("active", "open");
                link.parentElement.parentElement.parentElement.parentElement.parentElement.classList.add("active", "open");
               // break;
            }
           
        });


       
    })


</script>
