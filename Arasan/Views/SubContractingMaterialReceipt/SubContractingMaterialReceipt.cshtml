                                </div>
                            </div>


                        <div style="height:30px;"></div>
                        <div class="row">
                           
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="inputName3" class="col-sm-4 control-label">Qty to be Recived<label class="Mandatory"></label></label>

                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(C => C.qtyrec, new { @id = "qtyrec", @class = "form-control", @TabIndex = 11 })<br />
                                    </div>

                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">

                                    <label for="inputName3" class="col-sm-4 control-label">Recived Qty<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(C => C.TotRecqty, new { @id = "TotRecqty", @class = "form-control", @TabIndex = 11 })<br />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">

                                        <br /> <label for="inputName3" class="col-sm-4 control-label">Entered By<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                            @Html.DropDownListFor(C => C.enterd, Model.assignList," -- Please Select -- ", new { @id = "ddlenterd", @Class = "form-control _select", @TabIndex = 6 })
                                    </div>
                                </div>
                            </div>
                         </div>


                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">

                                            <br /> <label for="inputName3" class="col-sm-4 control-label">Narration<label class="Mandatory"></label></label>
                                            <div class="col-sm-8">
                                                @Html.TextBoxFor(C => C.Narration, new { @id = "txtNarration", @class = "form-control", @TabIndex = 1 })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            <div style="height:30px;"></div>
                            <button type="submit" class="btn aqua m-t-xs bottom15-xs" id="btnSave">Submit</button>
                            <button class="btn btn-danger" type="button" causesvalidation="false" onclick="location.href='@Url.Action("ListSubContractingMaterialReceipt", "SubContractingMaterialReceipt")'">Cancel</button>


                    </form>
                    }
                </div>
            </div>
