    </div>
</div>
<script>
    $(function () {
        $('._select').select2();
    });
    $('.form_date').datetimepicker({
        //   language:  'fr',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        minView: 2,
        forceParse: 0
    });
    $('.form_datetime').datetimepicker({
        //language:  'fr',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        forceParse: 0,
        showMeridian: 1
    });
    function PopulateDropDown(dropDownId, list) {
        //alert(list);
        $(dropDownId).empty().append('<option selected="selected" value="0">Please select</option>');
        if (list != null && list.length > 0) {
            $(dropDownId).removeAttr("disabled");
            $.each(list, function () {
                $(dropDownId).append($("<option></option>").val(this['value']).html(this['text']));
            });
        }
        $(dropDownId).trigger("liszt:updated");
    }
    function changeDcDeatils(ItemId) {
       @* $.ajax({
            url: 'GetDCDetail',s
            data: { "ItemId": ItemId },
            type: "GET",
            success: function (result) {

                document.getElementById("txtAdd1").value = result.add1;

                document.getElementById("txtAdd2").value = result.add2;
                document.getElementById("txtCity").value = result.city;
            },
            error: function () {
                alert("Data Not Found");
            }
        });*@
        $.ajax({
            url: "GetPartyJSON",
            type: "post",
            data: { 'itemid': ItemId },
            cache: false,
            success: function (response) {
                var newlist;
                //alert(JSON.stringify(response));
                newlist = response.Ilst;
                PopulateDropDown("#ddlSupplier", response);
            },
            error: function () {
                debugger;
                alert("Please Select Dc No");
            }
        });

        document.getElementById("tbBooks").innerHTML = "";
        document.getElementById("tbBooks1").innerHTML = "";
        $.ajax({
            url: 'GetSubRecivedDetail',

            type: "post",
            data: { "ItemId": ItemId, },
            cache: false,
            success: function (list) {
                if (list != null && list.length > 0) {

                    list.forEach((item) => {

                        var index = $("#tbBooks").children("tr").length;

                        var ItemId = "<td><label style='width: 200px;'>" + item.item + "</label><input type='hidden' value=" + item.itemid + " id='txtitemid" + index + "' name='Contlilst[" + index + "].itemid'><input type='hidden' value=" + item.id + " id='txtID" + index + "' name='Contlilst[" + index + "].id'></td>";

                        var Unit = "<td><input class='form-control' id='txtUnit" + index + "' name='Contlilst[" + index + "].unit'  style='width: 90px;' type='text'  readonly='True'  value=" + item.unit + " /></td>";


                        var Qty = "<td><input class='form-control' id='txtqty" + index + "' name='Contlilst[" + index + "].qty'  style='width: 100px;' type='text' readonly='True' value=" + item.qty + " /></td>";





                        var rate = "<td><input class='form-control' id='txtrate" + index + "' name='Contlilst[" + index + "].rate'  onkeydown='return isNumeric2(event.keyCode,this.value)' style='width: 100px;' type='text' value=" + item.rate + " readonly='True' /></td>";

                        var Amount = "<td><input class='form-control' id='txtamount" + index + "' name='Contlilst[" + index + "].amount'  onkeydown='return isNumeric2(event.keyCode,this.value)' style='width: 120px; type='text' value=" + item.amount + " readonly='True' /></td>";






                        var removeCell = "<td><input id='Isvalid" + index + "' name='Contlilst[" + index + "].Isvalid' type='hidden' value='Y'><img  src='../Images/delete.png' onclick='removeRow1(" + index + ");'  style='cursor:pointer' /><input name='Contlilst.Index' type='hidden' value='" + index + "'  /></td>";

                        var newRow = "<tr class='chkitemlst'  style='height: 30px;border-bottom: 1px dashed #ddd;'  id='trBook" + index + "'>" + ItemId + Unit + Qty + rate + Amount + removeCell + "</tr>";
                        $("#tbBooks").append(newRow);

                    });

                }


            },
            error: function () {
                debugger;
                alert("Please Select Supplier");
            }
        });
        $.ajax({
            url: 'GetSubDeliverItemDetail',

            type: "post",
            data: { "ItemId": ItemId, },
            cache: false,
            success: function (list) {
                if (list != null && list.length > 0) {

                    list.forEach((item) => {

                        var index = $("#tbBooks1").children("tr").length;

                        var ItemId = "<td><label style='width: 200px;'>" + item.item + "</label><input type='hidden' value=" + item.itemid + " id='txtitemid" + index + "' name='SubMatlilst[" + index + "].itemid'><input type='hidden' value=" + item.drumno + " id='txtdrumno" + index + "' name='SubMatlilst[" + index + "].drumno'><input type='hidden' value=" + item.dqty + " id='txtdqty" + index + "' name='SubMatlilst[" + index + "].dqty'><input type='hidden' value=" + item.drate + " id='txtdrate" + index + "' name='SubMatlilst[" + index + "].drate'><input type='hidden' value=" + item.damount + " id='txtdamount" + index + "' name='SubMatlilst[" + index + "].damount'><input type='hidden' value=" + item.supid + " id='txtsupid" + index + "' name='SubMatlilst[" + index + "].supid'></td>";
                        var Drum = "<td><div class='fa-hover col-md-2 col-sm-4''> <a href='@Url.Action("DrumSelection", "SubContractingMaterialReceipt")?rowid=0&id=" + item.supid + "' class='fancybox' data-fancybox-type='iframe' ><img src='../Images/view_icon.png' /></a></div> </td>";

                        var Unit = "<td><input class='form-control' id='txtUnit" + index + "' name='SubMatlilst[" + index + "].unit'  style='width: 90px;' type='text'  readonly='True'  value=" + item.unit + " /></td>";


                        var Qty = "<td><input class='form-control' id='txtqty" + index + "' name='SubMatlilst[" + index + "].qty'  style='width: 100px;' type='text' readonly='True' value=" + item.qty + " /></td>";





                        var rate = "<td><input class='form-control' id='txtrate" + index + "' name='SubMatlilst[" + index + "].rate'  onkeydown='return isNumeric2(event.keyCode,this.value)' style='width: 100px;' type='text' value=" + item.rate + " readonly='True' /></td>";

                        var Amount = "<td><input class='form-control' id='txtamount" + index + "' name='SubMatlilst[" + index + "].amount'  onkeydown='return isNumeric2(event.keyCode,this.value)' style='width: 120px;' type='text' value=" + item.amount + " readonly='True' /></td>";






                        var removeCell = "<td><input id='Isvalid" + index + "' name='SubMatlilst[" + index + "].Isvalid' type='hidden' value='Y'><img  src='../Images/delete.png' onclick='removeRow(" + index + ");'  style='cursor:pointer' /><input name='SubMatlilst.Index' type='hidden' value='" + index + "'  /></td>";

                        var newRow = "<tr class='chkitemlst'  style='height: 30px;border-bottom: 1px dashed #ddd;'  id='trBook" + index + "'>" + ItemId + Drum + Unit + Qty + rate + Amount + removeCell + "</tr>";
                        $("#tbBooks1").append(newRow);

                    });

                }


            },
            error: function () {
                debugger;
                alert("Please Select DC Number");
            }
        });
    }
    function ItemChange(ItemId, i) {
    @* var locid = document.getElementById("ddlLoc").value;*@
            $.ajax({
                url: 'GetItemDetail',
                data: { "ItemId": ItemId },
                type: "GET",
                success: function (result) {

                    document.getElementById("txtunit" + i).value = result.unit;
                    document.getElementById("txtrate" + i).value = result.price;

            
                    },
                error: function () {
                    alert("Data Not Found");
                }
            });
    }
    function Item_Deatils(ItemId, j) {
    @* var locid = document.getElementById("ddlLoc").value;*@
            $.ajax({
                url: 'GetItem',
                data: { "ItemId": ItemId },
                type: "GET",
                success: function (result) {

                    document.getElementById("txtRunit" + j).value = result.unitid;
                    document.getElementById("txtRrate" + j).value = result.rate;


                },
                error: function () {
                    alert("Data Not Found");
                }
            });
    }
    function changeItem(ItemId) {
        $.ajax({
            url: 'GetWCDetail',
            data: { "ItemId": ItemId },
            type: "GET",
            success: function (result) {

                document.getElementById("txtWorkCenter").value = result.work;

            },
            error: function () {
                alert("Data Not Found");
            }
        });
    }

        function AddRow() {
            var opt = "";
            $.ajax({
                type: "POST",
                url: 'GetItemJSON',
                success: function (data) {

                    $.each(data, function () {
                        opt += "<option value=" + this.value + ">" + this.text + "</option>";
                    });
                    AppendRow(opt);
                }
            });
        }
        function removeRow(id) {
            var table = document.getElementById("dataTable");
            var rowCount = table.rows.length;
            var row = document.getElementById('trBook' + id);
            var j = 0;
            $('tr.chkitemlst').each(function () {
                if ($(this).css('display') !== 'none') {
                    j += 1;
                }
            });
            if (j > 1) {
                row.style.display = "none";
                document.getElementById('Isvalid' + id).value = "N";
            }
            else {
                alert("Cannot delete all the rows.")
            }
        }
