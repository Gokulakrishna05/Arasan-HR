@model Arasan.Models.SubContractingMaterialReceipt

@{
    Layout = "_PopupLayout";
    ViewData["Title"] = "View Pending";
    

}
<style>
    .newwidth {
        width: 120px;
    }
</style>


<div class="wrapper-content ">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="widgets-container">
                    <form>
                        
                        <div style="height:30px;"></div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="col-md-12" style="overflow-x:scroll; min-width:300px; min-height:300px;">
                                    <!-- / ibox-title -->
                                    <table class="table table-striped table-hover" id="dataTable">
                                        <thead>
                                            <tr>

                                                <th>Doc No</th>
                                                <th>Doc Date</th>

                                                <th>  Item </th>
                                                <th>  Qty </th>
                                                <th> RecQty </th>
                                                <th> PenQty </th>


                                            </tr>
                                        </thead>

                                        <tbody id="tbBooks">
                                            @{
                                                int i = 0;

                                                foreach (var item in Model.penlst.ToList())
                                                {

                                                    <tr id="trBook@(i)" class="chkitemlst" style="height: 30px;border-bottom: 1px dashed #ddd;">

                                                        <td>
                                                            @Html.DisplayFor(o => o.penlst[i].docNo)
                                                        </td>
                                                        <td> @Html.DisplayFor(o => o.penlst[i].docDate)  </td>
                                                        <td> @Html.DisplayFor(o => o.penlst[i].item) </td>
                                                        <td> @Html.DisplayFor(o => o.penlst[i].qty) </td>
                                                        <td> @Html.DisplayFor(o => o.penlst[i].recqty) </td>
                                                        <td> @Html.DisplayFor(o => o.penlst[i].penqty) </td>

                                                    </tr>
                                                    i++;
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>





                    </form>

                </div>
            </div>
        </div>
    </div>
</div>


<a class="fancybox1" rel="group" data-fancybox-type="iframe" href="#" style="display: none; width: 760px;"></a>
<a class="fancyboxe1" rel="group" data-fancybox-type="iframe" href="#" style="display: none; width: 760px;"></a>
<link rel="stylesheet" href="@Url.Content("~/css/jquery.fancybox.css?v=2.1.5")" type="text/css" media="screen" />
<script type="text/javascript" src="@Url.Content("~/css/jquery.fancybox.pack.js?v=2.1.5")"></script>
<script type="text/javascript">
    var fancybox123;
    jQuery(document).ready(function ($) {
        $(".fancybox").fancybox({
            width: '80%',
            height: '10%',
            arrows: false
        });
        fancybox123 = $.fancybox;
    });

    function close_popup() {
        fancybox123.close();
        $(".fancybox1").fancybox({
            width: '100%',
            height: '100%',
            arrows: false,
        });
        $(".fancybox1").eq(0).trigger('click');
    }
</script>
