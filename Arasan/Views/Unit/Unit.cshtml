@model Arasan.Models.Unit

@{
    ViewData["Title"] = "Index";
}
<div class="row wrapper border-bottom page-heading">
    <div class="col-lg-12">

        <ol class="breadcrumb">
            <li> <a href="#">Home</a> </li>
            <li> <a>Master</a> </li>
            <li class="active"> <strong>Unit Master</strong> </li>
        </ol>
    </div>
</div>


<div class="wrapper-content ">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="widgets-container">
                    <h5 style="font-size:14px">Unit Master</h5>
                    <hr>
                    <form>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">

                                    <label for="inputName3" class="col-sm-4 control-label">UnitId<label class="Mandatory"></label></label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(C => C.UnitId, new { @id = "txtUnitId", @class = "form-control", @TabIndex = 2 })
                                    </div>
                                </div>
                            </div>
                            </div>
                            <div class="row">
                            <div class="form-group">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <button class="btn aqua" type="submit">Save</button>
                                    <button class="btn btn-white" type="submit">Cancel</button>
                                </div>
                            </div>
                         </div>   
                        
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
